{
  "name": "fastify-playground-sandpack",
  "version": "1.0.0",
  "description": "Template to start a new repository on the hub",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "tsc --noEmit && eslint --ext .js,.ts,.jsx,.tsx src",
    "format": "npm run lint --fix & prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "preview": "npm run build && vite preview",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nearform/fastify-playground-sandpack.git"
  },
  "author": "Andy Smith <andy.smith@nearform.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nearform/fastify-playground-sandpack/issues"
  },
  "homepage": "https://nearform.github.io/fastify-playground-sandpack",
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "npm run format"
  }
}
